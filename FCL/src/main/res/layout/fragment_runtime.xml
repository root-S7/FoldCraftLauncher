                        android:layout_marginStart="10dp"/>

                </androidx.appcompat.widget.LinearLayoutCompat>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@android:color/darker_gray"/>

                <androidx.appcompat.widget.LinearLayoutCompat
